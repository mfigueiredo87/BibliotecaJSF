		<queryString language="SQL">
			<![CDATA[SELECT clinica.pessoa.nome,
	clinica.pessoa.`TIPO_PESSOA`,
	clinica.medico.codigo,
	clinica.pessoa.data_nascimento,
	clinica.pessoa.endereco,
	clinica.pessoa.celular,
	clinica.especialidade.nome,
	clinica.pessoa.cidade
FROM clinica.medico
	INNER JOIN clinica.pessoa ON 
	 clinica.medico.codigo = clinica.pessoa.codigo 
	INNER JOIN clinica.especialidade ON 
	 clinica.medico.codigo_especialidade = clinica.especialidade.codigo 
WHERE 
	 clinica.pessoa.`TIPO_PESSOA` = 'medico']]>
		</queryString>
		<field name="nome" class="java.lang.String"/>
		<field name="TIPO_PESSOA" class="java.lang.String"/>
		<field name="codigo" class="java.lang.Long"/>
		<field name="data_nascimento" class="java.sql.Date"/>
		<field name="endereco" class="java.lang.String"/>
		<field name="celular" class="java.lang.String"/>
		<field name="COLUMN_7" class="java.lang.String"/>
		<field name="cidade" class="java.lang.String"/>
	</subDataset>
	<parameter name="MEDICO_NOME" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT clinica.pessoa.celular,
	clinica.pessoa.cidade,
	clinica.pessoa.data_nascimento,
	clinica.pessoa.endereco,
	clinica.pessoa.estado,
	clinica.pessoa.nome,
	clinica.pessoa.sexo,
	clinica.pessoa.`TIPO_PESSOA`,
	clinica.especialidade.nome,
	clinica.medico.crm,
	clinica.medico.codigo
FROM clinica.medico
	INNER JOIN clinica.pessoa ON 
	 clinica.medico.codigo = clinica.pessoa.codigo 
	INNER JOIN clinica.especialidade ON 
	 clinica.medico.codigo_especialidade = clinica.especialidade.codigo 
WHERE 
	 clinica.pessoa.`TIPO_PESSOA` = 'medico' 
	 AND clinica.pessoa.nome LIKE  $P{MEDICO_NOME} ]]>
	</queryString>
